# ContadurIA - Example environment variables
# Never commit real secrets. Use .env.local for local dev.

# App
NEXT_PUBLIC_APP_NAME="ContadurIA"
NEXT_PUBLIC_APP_ENV="local"
NEXT_PUBLIC_APP_URL="http://localhost:3000"

# Database (future)
DATABASE_URL="postgresql://USER:PASSWORD@localhost:5432/contaduria?schema=public"

# Auth (future)
AUTH_SECRET="replace_me"
AUTH_URL="http://localhost:3000"

# Email (future)
EMAIL_PROVIDER="resend"
EMAIL_FROM="ContadurIA <no-reply@contaduria.com>"
RESEND_API_KEY="replace_me"
